{
  "name": "imdone-atom",
  "main": "./lib/imdone-atom",
  "version": "1.0.0",
  "description": "Issue Tracking for Tiny Teams and MVP's and a better way to manage TODOs, FIXMEs, HACKs, etc in your project.",
  "keywords": [
    "mvp",
    "tasks",
    "todo",
    "productivity",
    "issues"
  ],
  "activationCommands": {
    "atom-workspace": [
      "imdone-atom:tasks"
    ]
  },
  "repository": "https://github.com/imdone/imdone-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.5",
    "html5sortable": "^0.2.9",
    "imdone-core": "~1.2.0"
  }
}
