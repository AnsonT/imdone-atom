{
  "name": "imdone-atom",
  "main": "./lib/imdone-atom",
  "version": "2.0.9",
  "description": "Turn your TODO comments into a task-board and let imdone.io do your issue tracking so you can stay in the zone.",
  "keywords": [
    "todo",
    "fixme",
    "tasks",
    "productivity",
    "issues",
    "flow",
    "zone"
  ],
  "author": {
    "name": "Jesse Piascik",
    "email": "jesse@piascik.net",
    "url": "https://imdone.io"
  },
  "repository": "https://github.com/imdone/imdone-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.2.4 <2.0.0"
  },
  "dependencies": {
    "async": "^1.5.2",
    "atom-space-pen-views": "^2.0.5",
    "bootstrap-tokenfield": "^0.12.0",
    "debug": "^2.2.0",
    "engine.io": "^1.5.4",
    "engine.io-client": "^1.6.4",
    "fs-plus": "^2.8.1",
    "git-up": "^2.0.1",
    "imdone-core": "^1.3.9",
    "json-editor": "^0.7.28",
    "lodash": "^4.12.0",
    "loophole": "^1.1.0",
    "minimatch": "^3.0.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.11.0",
    "nedb": "^1.8.0",
    "pusher-js": "^3.0.0",
    "request": "^2.75.0",
    "sortablejs": "1.4.2",
    "superagent": "^2.3.0"
  },
  "deserializers": {
    "ImdoneAtomView": "createImdoneAtomView"
  },
  "providedServices": {
    "imdone": {
      "description": "Extend imdone with project and task level buttons",
      "versions": {
        "1.0.0": "provideService"
      }
    }
  },
  "coffeelintConfig": {
    "max_line_length": {
      "level": "warn",
      "value": 120
    }
  }
}
