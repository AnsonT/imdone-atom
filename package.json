{
  "name": "imdone-atom",
  "main": "./lib/imdone-atom",
  "version": "2.4.33",
  "description": "A kanban board with an invisible user interface. It's cards and lists are made from TODOs in your code, markdown and text files.",
  "keywords": [
    "kanban",
    "agile",
    "scrum",
    "todo",
    "fixme",
    "tasks",
    "productivity",
    "issues",
    "flow",
    "zone",
    "waffle",
    "github issues",
    "webhooks"
  ],
  "author": {
    "name": "Jesse Piascik",
    "email": "jesse@imdone.io",
    "url": "https://imdone.io"
  },
  "repository": "https://github.com/imdone/imdone-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.2.4 <2.0.0"
  },
  "scripts": {
    "build": "doctoc --github --title '### **Table of Contents**' README.md"
  },
  "dependencies": {
    "async": "^1.5.2",
    "atom-space-pen-views": "^2.0.5",
    "chokidar": "2.0.3",
    "chrono-node": "^1.3.5",
    "debug": "^2.2.0",
    "engine.io": "^1.5.4",
    "engine.io-client": "^1.6.4",
    "fs-plus": "^2.8.1",
    "git-up": "^2.0.1",
    "ignore": "^3.3.7",
    "imdone-core": "1.5.2",
    "json-editor": "^0.7.28",
    "keen-tracking": "^1.4.0",
    "laconic": "^0.2.2",
    "lodash": "^4.17.0",
    "loophole": "^1.1.0",
    "minimatch": "^3.0.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.21.0",
    "nedb": "^1.8.0",
    "node-schedule": "^1.3.0",
    "pusher-js": "^3.0.0",
    "regex-parser": "^2.2.9",
    "sortablejs": "1.7.0",
    "superagent": "^2.3.0",
    "superagent-no-cache": "^0.1.0",
    "wait-until": "0.0.2"
  },
  "deserializers": {
    "ImdoneAtomView": "createImdoneAtomView"
  },
  "providedServices": {
    "imdone": {
      "description": "Extend imdone with project and task level buttons",
      "versions": {
        "1.0.0": "provideService"
      }
    }
  },
  "coffeelintConfig": {
    "max_line_length": {
      "level": "warn",
      "value": 120
    }
  },
  "devDependencies": {
    "doctoc": "^1.3.0"
  }
}
